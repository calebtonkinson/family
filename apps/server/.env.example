# Database
DATABASE_URL=postgresql://user:password@your-neon-host.neon.tech/home_management?sslmode=require

# DBOS (uses same DATABASE_URL by default)
DBOS_DATABASE_URL=postgresql://user:password@your-neon-host.neon.tech/home_management?sslmode=require

# Auth (for validating tokens from frontend)
NEXTAUTH_SECRET=your-nextauth-secret-here

# AI Providers
ANTHROPIC_API_KEY=your-anthropic-api-key
OPENAI_API_KEY=your-openai-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key
RESEARCH_SEARCH_PROVIDER_ORDER=anthropic,openai,google,duckduckgo
OPENAI_WEB_SEARCH_MODEL=gpt-5
GOOGLE_WEB_SEARCH_MODEL=gemini-2.5-flash

# Push Notifications
VAPID_PUBLIC_KEY=your-vapid-public-key
VAPID_PRIVATE_KEY=your-vapid-private-key

# CORS - Allowed Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000

# Server
PORT=3001
